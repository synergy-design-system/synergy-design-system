{
  "author": {
    "name": "SICK Global UX Foundation",
    "url": "https://www.sick.com"
  },
  "description": "Fonts for Synergy, the SICK Design System",
  "devDependencies": {
    "ora": "^8.2.0",
    "postcss": "^8.5.6",
    "postcss-header": "^3.0.3",
    "postcss-import": "^16.1.1",
    "postcss-url": "^10.1.3"
  },
  "exports": {
    ".": {
      "default": "./src/SICKIntl/font.css",
      "import": "./src/SICKIntl/font.css"
    },
    "./SICKIntl": {
      "import": "./src/SICKIntl/font.css",
      "default": "./src/SICKIntl/font.css"
    },
    "./SICKIntl-inlined": {
      "import": "./dist/sickintl-inline.css",
      "default": "./dist/sickintl-inline.css"
    },
    "./src/*": {
      "default": "./src/*",
      "import": "./src/*"
    },
    "./package.json": {
      "default": "./package.json",
      "import": "./package.json"
    }
  },
  "files": [
    "dist",
    "src",
    "CHANGELOG.md",
    "NOTICE"
  ],
  "homepage": "https://synergy-design-system.github.io",
  "keywords": [
    "SDS",
    "SICK",
    "Synergy Design System",
    "SICK Fonts"
  ],
  "license": "MIT",
  "name": "@synergy-design-system/fonts",
  "repository": {
    "type": "git",
    "url": "https://github.com/synergy-design-system/synergy-design-system.git",
    "directory": "packages/fonts"
  },
  "scripts": {
    "build": "node scripts/build.js"
  },
  "type": "module",
  "version": "0.1.0"
}
